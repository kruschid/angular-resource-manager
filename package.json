{
  "name": "mbc-seite",
  "dependencies": {
    "body-parser": "^1.15.0",
    "feathers": "^1.3.0",
    "feathers-memory": "^0.6.0"
  },
  "devDependencies": {
    "bower": "^1.5.3",
    "browser-sync": "^2.10.0",
    "coffee-script": "^1.10.0",
    "del": "2.0.0",
    "gulp": "^3.9.0",
    "gulp-angular-protractor": "0.0.7",
    "gulp-coffee": "2.3.1",
    "gulp-concat": "^2.6.0",
    "gulp-install": "^0.5.0",
    "gulp-intercept": "^0.1.0",
    "gulp-jade": "1.1.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^2.1.0",
    "gulp-sequence": "^0.4.1",
    "gulp-shell": "^0.5.2",
    "gulp-sourcemaps": "^1.6.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.21",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coffee-preprocessor": "^0.3.0",
    "karma-jasmine": "^0.3.7",
    "karma-mocha-reporter": "^1.2.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "phantomjs-prebuilt": "^2.1.4"
  },
  "gcsj": {
    "run": [
      "node app.js"
    ],
    "browserSync": {
      "proxy": "localhost:3000"
    },
    "src": {
      "coffee": [
        "app/**/*.coffee",
        "app/**/*.litcoffee"
      ],
      "views": [
        "app/components/**/*.jade",
        "app/shared/**/*.jade"
      ],
      "mainView": "app/index.jade",
      "sass": "app/assets/css/styles.sass"
    },
    "dest": {
      "public": "public/",
      "js": "public/js/",
      "css": "public/css/",
      "fonts": "public/fonts/"
    },
    "libs": [
      "bower_components/angular/angular.js",
      "bower_components/angular-route/angular-route.js",
      "bower_components/lodash/dist/lodash.js",
      "bower_components/restangular/dist/restangular.js",
      "bower_components/angular-compass/js/angular-compass.js",
      "bower_components/angular-bootstrap/ui-bootstrap-tpls.js"
    ],
    "fonts": [
      "bower_components/bootstrap-sass/assets/fonts/bootstrap/*"
    ],
    "testConf": {
      "karma": "karma.conf.js",
      "protractor": "protractor.conf.js"
    }
  }
}
